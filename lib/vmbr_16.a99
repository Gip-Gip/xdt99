* VDP MULTI-BYTE READ 16 BITS
* use when running from 16 bit RAM or scratchpad RAM

       ref  vdpwa, vdprd

       even

vmbr:
       movb @ws_r0lb, @vdpwa
       andi r0, >3fff
       movb r0, @vdpwa
       nop                   ; prevent VDP overrun
!      movb @vdprd, *r1+
       dec  r2
       jne  -!

       rt
