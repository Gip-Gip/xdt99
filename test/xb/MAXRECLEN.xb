100 REM SUPPORTED REC LENS

110 L$="" :: FOR I=64 TO 127 :: L$=L$&CHR$(I) :: NEXT I :: L$=L$&L$
120 PRINT LEN(L$)

130 INPUT "START (128)?":S$
140 IF S$="" THEN S=128 ELSE S=VAL(S$)

200 FOR L=S TO 255
210 PRINT L

300 OPEN #1:"DSK2.QV"&STR$(L),DISPLAY,OUTPUT,VARIABLE L
310 PRINT #1:L$;SEG$(L$,1,L-128)
320 CLOSE #1

400 OPEN #1:"DSK2.QV"&STR$(L),DISPLAY,INPUT,VARIABLE L
410 INPUT #1:I$  ! MAX SUPPORTED LENGTH IS 156
420 PRINT I$
430 CLOSE #1

500 NEXT L
